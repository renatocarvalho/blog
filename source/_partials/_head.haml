%meta{:charset => "utf-8"}/
%meta{:content => "IE=edge;chrome=1", "http-equiv" => "X-UA-Compatible"}/
%meta{:content => "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0", :name => "viewport"}/
%meta{:content => "yes", :name => "apple-mobile-web-app-capable"}/
%meta{:property => "og:title", :content => "Front-end design and engineering."}/
%meta{:property => "og:type", :content => "website"}/
%meta{:property => "og:url", :content => "http://blog.jamesflorentino.com"}/
%meta{:content => "http://blog.jamesflorentino.com/images/ogp.jpg", :property => "og:image"}/
%meta{:content => "image/png", :property => "og:image:type"}/
%meta{:content => "400", :property => "og:image:width"}/
%meta{:content => "300", :property => "og:image:height"}/
%meta{:content => "A blog about front-end design and development. I talk about art, design, programming, and technology in general.", :property => "og:description"}/

%title= "#{title} - James Florentino"
= favicon_tag 'favicon.ico'
= stylesheet_link_tag 'all'

%script{ :src => 'http://code.jquery.com/jquery-1.7.2.min.js', :type => 'text/javascript' }
= javascript_include_tag 'all'
= partial '_partials/analytics'
